    "test": "vitest --run",
    "test:watch": "vitest",
    "build": "tsc --noEmit -p tsconfig.json && parcel build",
    "build:debug": "parcel build --no-optimize",
    "watch": "parcel watch",
    "check": "tsc --noEmit -p tsconfig.json",
    "prepublish": "tsc --noEmit -p tsconfig.json && parcel build"
