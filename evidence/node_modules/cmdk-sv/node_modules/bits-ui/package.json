{
  "name": "bits-ui",
  "version": "0.9.9",
  "license": "MIT",
  "repository": "github:huntabyte/bits-ui",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@huntabyte/mdsvex": "0.16.5",
    "@melt-ui/pp": "^0.1.3",
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-vercel": "^3.0.3",
    "@sveltejs/kit": "^1.27.4",
    "@sveltejs/package": "^2.2.2",
    "@svitejs/changesets-changelog-github-compact": "^1.1.0",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/svelte": "^4.0.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest-axe": "^3.5.8",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "autoprefixer": "^10.4.16",
    "clsx": "^2.0.0",
    "concurrently": "^8.2.2",
    "contentlayer": "^0.3.4",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.35.0",
    "esm-env": "^1.0.0",
    "hast-util-to-html": "^9.0.0",
    "jest-axe": "^8.0.0",
    "jsdom": "^22.1.0",
    "lucide-svelte": "^0.292.0",
    "phosphor-svelte": "^1.3.0",
    "postcss": "^8.4.31",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "publint": "^0.2.5",
    "radix-icons-svelte": "^1.2.1",
    "rehype-pretty-code": "^0.10.0",
    "remark-gfm": "^4.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "shiki": "^0.14.5",
    "svelte": "^4.2.3",
    "svelte-check": "^3.6.0",
    "svelte-sequential-preprocessor": "^2.0.1",
    "svelte-wrap-balancer": "^0.0.4",
    "tailwind-merge": "^1.14.0",
    "tailwind-variants": "^0.1.14",
    "tailwindcss": "^3.3.5",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "unist-builder": "^4.0.0",
    "unist-util-visit": "^5.0.0",
    "vite": "^4.5.0",
    "vitest": "0.33.0"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "@melt-ui/svelte": "0.61.2",
    "nanoid": "^5.0.3"
  },
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "packageManager": "pnpm@8.6.3",
  "scripts": {
    "dev": "concurrently \"pnpm:dev:content\" \"pnpm:dev:svelte\"",
    "dev:content": "contentlayer dev",
    "dev:svelte": "vite dev",
    "build": "contentlayer build &&  vite build && pnpm run package",
    "build:content": "contentlayer build",
    "build:content-cachebust": "contentlayer build --clearCache",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "check": "pnpm build:content && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test": "vitest",
    "release": "changeset publish",
    "changeset": "changeset"
  }
}